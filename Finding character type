// If-Else to find character type
#include <iostream>
using namespace std;
int main()
{
    char c;
    cout<<"Enter any character: "; cin>>c;
    if(c>=48 && c<=57) cout<<"Digit";
    else if(c>=65 && c<=90) cout<<"Upper case";
    else if(c>=97 && c<=122) cout<<"Lower case";
    else cout<<"Other symbol";
    cout<<endl;
    return 0;
}
