//Program to find the sum of numbers in a given range
#include <iostream>
using namespace std;
int main()
{
	long sum=0;
	int start;
	int end;
	cout << "**Program to find the sum of numbers in any given range**"<<endl;
	cout << "Enter the starting number of range: ", cin >> start;
	cout << "Enter the final number of range: ", cin >> end;
	
	for (int i = start; i <= end; i++)
	{
		sum = sum + i;

	}
	cout << "The sum of numbers starting from " <<start<<" and ending at "<<end<<" is "<< sum << endl;
	return 0;
}
